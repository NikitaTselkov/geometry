<UserControl x:Class="geometry.Views.UserControls.FindInputValue"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:UC="clr-namespace:geometry.Views.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <StackPanel>

            <Button x:Name="ButtonValueFind" Content="Add +" Style="{StaticResource Spawn}" />

            <UC:FindSetData x:Name="Setting" IsPressedFind="{Binding ElementName=ButtonValueFind, Path=IsPressed}" Visibility="Collapsed"/>

        </StackPanel>
        
    </Grid>
</UserControl>
